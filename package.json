    "babel-loader": "^7.1.3",
    "babel-preset-es2015": "^6.24.1",
    "concurrently": "^3.5.1",
    "css-loader": "^0.28.11",
    "mini-css-extract-plugin": "^0.4.0",
    "node-sass": "^4.9.2",
    "sass-loader": "^7.0.1",
